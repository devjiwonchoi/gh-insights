    "@types/node": "^20.8.4",
    "@types/supertest": "^2.0.14",
    "dotenv-cli": "^7.3.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "typescript": "^5.2.2"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
